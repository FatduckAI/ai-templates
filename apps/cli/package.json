{
  "name": "@fatduckai/cli",
  "version": "0.2.4",
  "description": "CLI tool for managing FatDuck AI prompts and tools",
  "type": "module",
  "bin": {
    "fatduckai": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "test": "bun test",
    "start": "bun run dist/index.js",
    "dev": "bun run src/index.ts",
    "prepack": "node ./scripts/prepack.js",
    "postpack": "node ./scripts/postpack.js"
  },
  "dependencies": {
    "@fatduckai/core": "workspace:*",
    "@types/glob": "^8.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "ora": "^8.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.2.2",
    "bun-types": "latest"
  },
  "publishConfig": {
    "access": "public",
    "dependencies": {
      "@fatduckai/core": "^0.2.0"
    }
  }
}
